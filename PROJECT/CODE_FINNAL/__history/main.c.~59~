#include <library_project.c>

void main(){

//! setup enviroments
   setup_initialize();

//! setup ds1307 rtc
   ds1307_setup();

//! setup lcd display
   lcd_setup();

//! display info include: name project and name university
   log_lcd_initialize();

   while(true){
      ds1307_read_time(8);
      ds18b20_read_temp();
      lcd_setCursor(0,0);
      lcd_data(dt_ng/10 + 0x30);
      lcd_data(dt_ng%10 + 0x30);
      lcd_setCursor(0,1);
      lcd_data(dt_ng/10 + 0x30);
      lcd_data(dt_ng%10 + 0x30);
   }
}
