#include <thu_vien_do_an_1.c>
si16 temp;
void main(){
   
   set_up_port();
   
   lcd_init();
   lcd_log_info(1,enable);
   ds18b20_khoi_tao();
   ds18b20_doc_rom(1);
   while(true){
      if(ds18b20_read_temp(&temp))
         ds18b20_lcd_data(temp,0,0);
      else ds18b20_no_ds18b(0,0);
      
      if(touch_present())
         ds18b20_lcd_data(ds18b20_read_temp(),1,1);
      else ds18b20_no_ds18b(0,1);
   }
}
